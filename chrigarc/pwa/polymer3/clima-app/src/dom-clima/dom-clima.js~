import {html, PolymerElement} from '@polymer/polymer/polymer-element.js';

/**
 * @customElement
 * @polymer
 */
class DomClima extends PolymerElement {
  static get template() {
    return html`
      <style>
        :host {
          display: block;
        }
      </style>
      <h1>Componente activado</h1>	  
    `;
  }
  static get properties() {
    return {
    };
  }
}

window.customElements.define('dom-clima', DomClima);
